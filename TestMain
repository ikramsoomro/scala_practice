package org.test.scala.practice

import scala.util.control.Breaks._

object TestMain {

  def main(args: Array[String]) : Unit = {
    
    
    println("test")
    
    //for (i <- 0 to 9) 
    //  println(i)
    
    
    //(0.to(9)).foreach(i => println(i))
    
    //(0 to 9).foreach(println(_))
    
    
    breakable {
      for(i <- 0 to 100){
        println(i)
        if(i == 11)
        break()
      }
    }
    
    
    
  }
}
